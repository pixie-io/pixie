{
  "variables": [
    {
      "name": "start",
      "type": "PX_STRING",
      "description": "The relative start time of the window. Current time is assumed to be now",
      "defaultValue": "-5m"
    },
    {
      "name": "pod",
      "type": "PX_STRING",
      "description": "The full/partial name of the pod to filter by for CQL request. Format: ns/pod_name",
      "defaultValue": ""
    }
  ],
  "globalFuncs": [
    {
      "outputName": "LET",
      "func": {
        "name": "pod_cql_let",
        "args": [
          {
            "name": "start",
            "variable": "start"
          },
          {
            "name": "pod",
            "variable": "pod"
          }
        ]
      }
    }
  ],
  "widgets": [
    {
      "name": "p50 Latency",
      "position": {
        "x": 0,
        "y": 0,
        "w": 4,
        "h": 3
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "latency_p50",
            "series": "k8s",
            "stackBySeries": false,
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "P50 Latency"
        },
        "xAxis": null
      }
    },
    {
      "name": "p90 Latency",
      "position": {
        "x": 4,
        "y": 0,
        "w": 4,
        "h": 3
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "latency_p90",
            "series": "k8s",
            "stackBySeries": false,
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "P90 Latency"
        },
        "xAxis": null
      }
    },
    {
      "name": "Request Throughput",
      "position": {
        "x": 0,
        "y": 3,
        "w": 6,
        "h": 3
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "request_throughput",
            "series": "k8s",
            "stackBySeries": false,
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "RPS"
        },
        "xAxis": null
      }
    },
    {
      "name": "Request Error Rate",
      "position": {
        "x": 8,
        "y": 0,
        "w": 4,
        "h": 3
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "error_rate",
            "series": "k8s",
            "stackBySeries": false,
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "Error Rate (%)"
        },
        "xAxis": null
      }
    },
    {
      "name": "Request Latency Histogram",
      "func": {
        "name": "latency_histogram",
        "args": [
          {
            "name": "start",
            "variable": "start"
          },
          {
            "name": "pod",
            "variable": "pod"
          }
        ]
      },
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.HistogramChart",
        "histogram": {
          "value": "request_latency",
          "prebinCount": "count",
          "maxbins": 10,
          "minstep": 50.0
        },
        "xAxis": {
          "label": "Request Latency"
        },
        "yAxis": {
          "label": "# of requests"
        }
      },
      "position": {
        "x": 6,
        "y": 3,
        "w": 6,
        "h": 3
      }
    },
    {
      "name": "Summary",
      "position": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 3
      },
      "func": {
        "name": "summary_cql_let",
        "args": [
          {
            "name": "start",
            "variable": "start"
          },
          {
            "name": "pod",
            "variable": "pod"
          }
        ]
      },
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.Table"
      }
    }
  ]
}
