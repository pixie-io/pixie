        "args": [
          {
            "name": "start_time",
            "variable": "start_time"
          },
          {
            "name": "namespace",
            "variable": "namespace"
          }
        ]
      },
      "displaySpec": {
        "@type": "pixielabs.ai/pl.vispb.Table"
      }
    },
    {
      "name": "HTTP Request Throughput",
      "position": {
        "x": 4,
        "y": 0,
        "w": 4,
        "h": 3
      },
      "globalFuncOutputName": "inbound_service_let",
      "displaySpec": {
        "@type": "pixielabs.ai/pl.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "request_throughput",
            "mode": "MODE_LINE",
