{
  "variables": [
    {
      "name": "start_time",
      "type": "PX_STRING",
      "description": "The relative start time of the window. Current time is assumed to be now",
      "defaultValue": "-5m"
    },
    {
      "name": "requesting_pod",
      "type": "PX_POD",
      "description": "The name of the pod sending the request. Format: ns/svc_name",
      "defaultValue": ""
    },
    {
      "name": "responding_pod",
      "type": "PX_POD",
      "description": "The name of the pod receiving the request. Format: ns/svc_name",
      "defaultValue": ""
    }
  ],
  "globalFuncs": [
    {
      "outputName": "LET",
      "func": {
        "name": "pod_edge_let",
        "args": [
          {
            "name": "start_time",
            "variable": "start_time"
          },
          {
            "name": "requesting_pod",
            "variable": "requesting_pod"
          },
          {
            "name": "responding_pod",
            "variable": "responding_pod"
          }
        ]
      }
    }
  ],
  "widgets": [
    {
      "name": "HTTP Request Latency",
      "position": {
        "x": 0,
        "y": 0,
        "w": 4,
        "h": 3
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "latency_p50",
            "mode": "MODE_LINE"
          },
          {
            "value": "latency_p90",
            "mode": "MODE_LINE"
          },
          {
            "value": "latency_p99",
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "latency"
        },
        "xAxis": null
      }
    },
    {
      "name": "HTTP Request Throughput",
      "position": {
        "x": 4,
        "y": 0,
        "w": 4,
        "h": 3
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "request_throughput",
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "request throughput"
        },
        "xAxis": null
      }
    },
    {
      "name": "HTTP Request Error Rate",
      "position": {
        "x": 8,
        "y": 0,
        "w": 4,
        "h": 3
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "error_rate",
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "Error Rate"
        },
        "xAxis": null
      }
    },
    {
      "name": "HTTP Data Sent/Received",
      "position": {
        "x": 0,
        "y": 3,
        "w": 4,
        "h": 3
      },
      "globalFuncOutputName": "LET",
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.TimeseriesChart",
        "timeseries": [
          {
            "value": "resp_data_throughput",
            "mode": "MODE_LINE"
          },
          {
            "value": "req_data_throughput",
            "mode": "MODE_LINE"
          }
        ],
        "title": "",
        "yAxis": {
          "label": "http throughtput"
        },
        "xAxis": null
      }
    },
    {
      "name": "Status Code Distribution",
      "position": {
        "x": 4,
        "y": 3,
        "w": 4,
        "h": 3
      },
      "func": {
        "name": "http_code_agg",
        "args": [
          {
            "name": "start_time",
            "variable": "start_time"
          },
          {
            "name": "requesting_pod",
            "variable": "requesting_pod"
          },
          {
            "name": "responding_pod",
            "variable": "responding_pod"
          }
        ]
      },
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.BarChart",
        "bar": {
          "value": "count",
          "label": "http_resp_status",
          "horizontal": true
        },
        "xAxis": {
          "label": "# of requests"
        },
        "yAxis": {
          "label": "HTTP Status Code"
        }
      }
    },
    {
      "name": "Request Latency Histogram",
      "position": {
        "x": 8,
        "y": 3,
        "w": 4,
        "h": 3
      },
      "func": {
        "name": "latency_histogram",
        "args": [
          {
            "name": "start_time",
            "variable": "start_time"
          },
          {
            "name": "requesting_pod",
            "variable": "requesting_pod"
          },
          {
            "name": "responding_pod",
            "variable": "responding_pod"
          }
        ]
      },
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.HistogramChart",
        "histogram": {
          "value": "request_latency",
          "prebinCount": "count",
          "maxbins": 10,
          "minstep": 50.0
        },
        "xAxis": {
          "label": "Request Latency"
        },
        "yAxis": {
          "label": "# of requests"
        }
      }
    },
    {
      "name": "HTTP Traffic",
      "position": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 3
      },
      "func": {
        "name": "http_requests",
        "args": [
          {
            "name": "start_time",
            "variable": "start_time"
          },
          {
            "name": "requesting_pod",
            "variable": "requesting_pod"
          },
          {
            "name": "responding_pod",
            "variable": "responding_pod"
          }
        ]
      },
      "displaySpec": {
        "@type": "types.px.dev/px.vispb.Table"
      }
    }
  ]
}
